

<div class="jumbotron text-center">
  <h2> Analytics page for "<%= @registered_application.name %>" </h2>  -
</div>
<div class="container">
  <div class="col-md-4">
    <p> URL : <%= @registered_application.url %></p>
  </div>
  <div class="col-md-4">
    <% @events.each do |event| %>
    <h6>Currently tracking <%= "#{event.last.length}" %> events: </h6>

    <p> <%= event.first %> </p>
    <% end %>
  </div>
</div>

 <div class="nav nav-tabs">
  <p> <%= link_to "Delete this registered application", @registered_application, method: :delete, data: {confirm: "Are you sure you want to delete this application?"} %> </p>
  <p> <%= link_to "View your registered applications", registered_applications_path %> </p>
 </div>
